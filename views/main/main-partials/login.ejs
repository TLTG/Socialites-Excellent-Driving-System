<section id="login" class="home1">
    <div class="limiter limiter1">
        <div class="container-login100">
            <div class="wrap-login100">
                <form class="login100-form validate-form">
                    <span class="login100-form-title">Socialites Excellent Driving</span>
                    <center><div class="separator"></div>
                        <h5 style="color: #5D0000;">Login as:</h5></center>
                    
                    <div class="contLoginBtn">
                        <button class="loginBtn" onclick="logStud()">STUDENT</button>
                    </div>
                    <div class="contLoginBtn">
                        <button class="loginBtn" onclick="logInst()">INSTRUCTOR</button>
                    </div>
                </form>
            </div>
        </div>
    </div>

    <div class="limiter limiter2">
        <div class="container-login100">
            <div class="wrap-login100">
                <form class="login100-form validate-form">
                    <span class="login100-form-title">Socialites Excellent Driving</span>
                    <center><h5 class="h5stud">- STUDENT -</h5><h5 class="h5inst">- INSTRUCTOR -</h5><div class="separator"></div></center>

                    <div class="wrap-input100 validate-input" data-validate = "Username/Email is required">
                        <input id="usernameTxt" class="input100" type="text" name="email" placeholder="Username/Email">
                        <span class="focus-input100"></span>
                        <span class="symbol-input100">
                            <i class="fa fa-user" aria-hidden="true"></i>
                        </span>
                    </div>

                    <div class="wrap-input100 validate-input" data-validate = "Password is required">
                        <input id="passwordTxt" class="input100" type="password" name="pass" placeholder="Password">
                        <span class="focus-input100"></span>
                        <span class="symbol-input100">
                            <i class="fa fa-lock" aria-hidden="true"></i>
                        </span>
                    </div>
                    
                    <div class="container-login100-form-btn">
                        <button class="login100-form-btn">Login</button>
                    </div>

                    <div class="text-center p-t-12">
                        <span class="txt1">Forgot</span>
                        <a class="txt2" href="#">Username / Password?</a><br>
                    </div>

                    <div class="text-center p-t-12">
                        <br><a class="txt2" onclick="gobackLogin()" style="cursor: pointer;">Go Back</a>
                    </div>
                </form>
            </div>
        </div>
    </div>
</section><!--End of home section -->
