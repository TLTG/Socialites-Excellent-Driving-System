<!DOCTYPE html>
<html lang="en">
    <head>
        <title>Branch | Socialites Excellent Driving</title>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta http-equiv="X-UA-Compatible" content="ie=edge">
        <link rel="icon" type="image/png" sizes="96x96" href="assets/images/icon.png">
        <!-- CSS imports -->
        <% include ../utils/adminCSS %>
    </head>

    <body class="fix-header fix-sidebar">
        <!-- Preloader - style you can find in spinners.css -->
        <div class="preloader">
            <svg class="circular" viewBox="25 25 50 50">
            <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10" /> 
            </svg>
        </div>

        <!-- WRAPPER -->
        <div id="wrapper">
            <% include ../admin/partials/header %> <!-- HEADER CODES -->
            <% include ../admin/partials/sidebar %> <!-- SIDEBAR CODES -->
            <% include ./modals %> 

            <!-- MAIN -->
            <div class="page-wrapper">
                <!-- MAJOR PARTIALS -->
                <% include ./main-partials/branchDashboard %>
                <% include ./main-partials/branchStudent %>
                <% include ./main-partials/branchInstructor %>
                <% include ./main-partials/branchVehicle %>
                <% include ./main-partials/branchPayment %>
                
                <% include ./main-partials/branchEnrolment %>
                <% include ./main-partials/branchLicense %>
                
                <% include ./main-partials/branchReports %>
                <% include ./main-partials/branchAccount %>
                <% include ./main-partials/branchCertificate %>

                <!-- MINOR PARTIALS -->
                <% include ./minor-partials/profileBranch %>
                <% include ./minor-partials/scheduleBranch %>
                <% include ./minor-partials/schedCanceledBranch %>
                <% include ./minor-partials/view-studentBranch %>
                <% include ./minor-partials/view-instructorBranch %>
                <% include ./minor-partials/schedListView %>
                <% include ./minor-partials/suspendClass %>
                <% include ./minor-partials/todaySched %>
                <% include ./minor-partials/transferReq %>
            </div>
            <!-- END MAIN -->
            <% include ../admin/partials/footer %> 
            <!-- FOOTER CODES -->

            <!-- JS imports -->
            <% include ../utils/branchJS %>
        </div>
    </body>
</html>