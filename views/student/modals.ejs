<!-- View Evaluation Student Modal -->
<div class="modal fade" id="viewEvalStudModal" tabindex="-1" role="dialog" aria-hidden="true" aria-labelledby="viewEvalStudModal">
  <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-body">
        <center>
            <h2>PERFORMANCE EVALUATION</h2>
            <h6>Grades for every lesson are tallied and computed to analyze the student's performance for the whole course.</h6>
        </center>
        <hr class="hrCustom">
        <div class="stot">
          <form>
            <div class="row">
              <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                <% 
                    var pad = "0000";
                    var eval;
                    var course = "CRS-"+ (evalStud[0].carType.toUpperCase() + pad.substring(0,pad.length - (evalStud[0].courseID+"").length)) + evalStud[0].courseID;
                    var html = "<h6>Course: <b><span class='evalCrs'>" + (course) + "</span></b></h6>";
                    html += "<h6>Overall Grade: <b><span class='overallGrade'>"+ evalStud[0].grade +"</span>&nbsp;(<span class='overallGrPerc'>"+ (evalStud[0].grade)*2 +"%</span>)</b></h6>";
                    html += "<h6>Evaluation: <b><span class='evalGrade'>"+ (evalStud[0].grade <= 24 ? 'NEEDS TO EXTEND' : ((evalStud[0].grade <= 34 && evalStud[0].grade >= 25) ? 'NEEDS PRACTICE' : 'PASSED')) +"</span></b></h6><br><br>";
                    html += "<h6>Comment:</h6><textarea class='enrTxtArea' id='commentInstEval' readonly style='font-size: 14px' rows='3'>"+ evalStud[0].comment +"</textarea><br><br>";
                    html += "<h6>Evaluated by: <b><span class='instEvaluator'>"+ evalStud[0].fullname +"</span></b></h6>";
                    html += "<h6>Date Evaluated: <b><span class='evalDate'>"+ (Date.parse(evalStud[0].dateEvaluated).toString("MMM dd, yyyy")) +"</span></b></h6>";
                %>
                <%-html%>
              </div>
              <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                <h6><b>Evaluation Basis:</b></h6>
                <h6><b>35-50</b> - Passed</h6>
                <h6><b>25-34</b> - Needs Practice</h6>
                <h6><b>0-24&nbsp;&nbsp; </b> - Needs To Extend</h6>
              </div>
            </div>
          </form>
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn100 btn13 btn-inverse" data-dismiss="modal">Done</button>
      </div>
    </div>
  </div>
</div>

