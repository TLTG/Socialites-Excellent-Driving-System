                                            <option>Tuesday</option>
                                            <option>Wednesday</option>
                                            <option>Thursday</option>
                                            <option>Friday</option>
                                            <option>Saturday</option>
                                            <option>Sunday</option>
                                        </select>
                                </div>
                                <div class="col-md-6 col-lg-6 col-sm-6 col-xs-6">
                                    <span class="fa fa-user fa-red fa-rightB">&nbsp;&nbsp;</span>
                                        <span id="vehiInst"> Harry James Potter</span>
                                        <select class="customTxtInput2 input-flat" id="editVehiInst">
                                            <option>Harry James Potter</option>
                                            <option>Dobby</option>
                                            <option>Bellatrix Lestrange</option>
                                            <option>Merula Synide</option>
                                            <option>Clarissa Fray</option>
                                            <option>---</option>
                                        </select><br>
                                </div>
                            </div><br><br><hr>
                            <div class="row">
                                <div class="col-md-6 col-lg-6 col-sm-6 col-xs-6">
                                    <h4 class="card-title">Defect(s):</h4>
                                </div>
                                <div class="col-md-6 col-lg-6 col-sm-6 col-xs-6">
                                    <i class="ti-plus tiV" id="btnAddDefVehi" onclick="addDefVehi('a')" data-toggle="tooltip" data-placement="bottom" title="Add Defect(s)"></i>
                                </div>
                            </div>
                            <div class="table-responsive">
                                <table class="table tblVehicleDef table-hover">
                                    <thead>
                                        <tr>
                                            <th>Part</th>
                                            <th>Damage</th>
                                            <th>Significance</th>
                                            <th>Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>Tires</td>
                                            <td>Deflated</td>
                                            <td><span class="text-warning"></span>3</td>
                                            <td><i class="ti-check tiDef" id="btnDoneDefVehi" onclick="doneDefVehi()" data-toggle="tooltip" data-placement="bottom" title="Fixed Defect"></i>
                                                <i class="ti-close tiDef1" id="btnCancDefVehi" onclick="cancDefVehi()" data-toggle="tooltip" data-placement="bottom" title="Cancel/Remove"></i></td>
                                        </tr>
                                        <tr>
                                            <td>Brake</td>
                                            <td>Broke</td>
                                            <td><span class="text-danger"></span>5</td>
                                            <td><i class="ti-check tiDef" id="btnDoneDefVehi" onclick="doneDefVehi()" data-toggle="tooltip" data-placement="bottom" title="Fixed Defect"></i>
                                                <i class="ti-close tiDef1" id="btnCancDefVehi" onclick="cancDefVehi()" data-toggle="tooltip" data-placement="bottom" title="Cancel/Remove"></i></td>
                                        </tr>
                                        <tr>
                                            <td>Plate Number</td>
                                            <td>Missing</td>
                                            <td><span class="text-danger"></span>5</td>
                                            <td><i class="ti-check tiDef" id="btnDoneDefVehi" onclick="doneDefVehi()" data-toggle="tooltip" data-placement="bottom" title="Fixed Defect"></i>
                                                <i class="ti-close tiDef1" id="btnCancDefVehi" onclick="cancDefVehi()" data-toggle="tooltip" data-placement="bottom" title="Cancel/Remove"></i></td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div><br>
                            <button type="button" id="btnDoneEditVehi" onclick="doneEditVehi('a')" class="btn btn-success m-b-10 m-l-5">Save Changes</button>
