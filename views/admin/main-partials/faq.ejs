<div class="view-faq viewDiv">
    <!-- Bread crumb -->
    <div class="row page-titles">
        <div class="col-md-5 align-self-center">
            <h3 class="text-primary">Frequently Asked Questions</h3> 
        </div>
        <div class="col-md-7 align-self-center">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a onclick="dashboard()">Home</a></li>
                <li class="breadcrumb-item active"><a>Frequently Asked Questions</a></li>
            </ol>
        </div>
    </div>
    <!-- End Bread crumb -->
    <!-- Container fluid  -->
    <div class="container-fluid">
        <!-- Start of Card  -->
        <div class="studentCard">
            <div class="row">
                <div class="col-md-6 col-lg-6 col-sm-12 col-xs-12">
                    <div>
                        <label style="float: left;">Label:</label>
                        <select id="faqLabelSel" name="faqLabelSel" class="enrCivStatus1" style="width: 80%"></select>
                    </div>
                </div>
                <div class="col-md-6 col-lg-6 col-sm-12 col-xs-12">
                    <div class="row">
                        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
                            <button type="button" onclick="addFaqLblModal()" class="btn btn100p btn-inverse m-b-10 m-l-5">Add Label</button>
                        </div>
                        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
                            <button type="button" onclick="editFaqLblModal()" class="btn btn100p btn-inverse m-b-10 m-l-5">Edit Label</button>
                        </div>
                        <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4">
                            <button type="button" onclick="deleteFaqLblModal()" class="btn btn100p btn-inverse m-b-10 m-l-5">Delete Label</button>
                        </div>
                    </div>
                </div>
            </div>
            <hr class="hrCustom">
            <div class="card branchCard">
                <div class="row">
                    <div class="col-md-6 col-lg-6 col-sm-12 col-xs-12">
                        <div>
                            <h3>List of FAQ (<span class="faqDisplaySpan"></span>)</h3>
                        </div>
                    </div>
                    <div class="col-md-6 col-lg-6 col-sm-12 col-xs-12">
                        <div class="pull-right">
                            <button type="button" onclick="addFaqModal()" class="btn btn100p btn-success m-b-10 m-l-5" title="Add FAQ under this label"><span class="ti ti-plus"></span></button>
                        </div>
                    </div>
                </div>
                <div class="table-responsive">
                    <table class="table tblFaq tblRecSched">
                        <thead>
                            <tr>
                                <th width="35%">Question</th>
                                <th width="55%">Answer</th>
                                <th width="10%">Actions</th>
                            </tr>
                        </thead>
                        <tbody id="faqTbl"></tbody>
                    </table>
                </div>
                <div class="noFaqDiv"><h5 style="text-align: center">There are no listed FAQs under this label.</h5></div>
            </div>
            <hr class="hrCustom">
        </div><!-- End of Card  -->
    </div>
    <!-- End Container fluid  -->
</div>